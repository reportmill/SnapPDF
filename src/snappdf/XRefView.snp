    <Label Name="NameLabel">
      <font name="Arial Bold" size="32" />
    </Label>
  </RowView>
  <TabView name="TabView" GrowHeight="true">
    <ScrollView title="Graphics">
      <BoxView FillWidth="false" FillHeight="false">
