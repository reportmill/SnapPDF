    </ScrollView>
    <SplitView name="BrowserSplit" Vertical="true" title="Structure">
      <BoxView Padding="8,4,4,4" FillWidth="true" FillHeight="true">
        <BrowserView name="BrowserView" PrefHeight="360" />
      </BoxView>
    </SplitView>
  </TabView>
</ColView>
